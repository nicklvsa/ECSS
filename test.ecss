$some_var: green;
$hello: 25px;

.some_class { 
    text-align: center;
    background-color: $some_var; 
    @extend<class>: cool_class;
}

.cool_class {
    transform: rotate(360deg);
    @extend<id>: example;
}

.other {
    color: white;
    font-weight: bold;
}

#example {
    max-width: 100%;
    @extend<class>: other;
}

#some_id {
    font-size: $hello;
}

